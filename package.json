    "url": "https://github.com/Gael22/shoppinglist/issues"
  },
  "homepage": "https://github.com/Gael22/shoppinglist#readme",
  "dependencies": {
    "@types/reactstrap": "^8.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "concurrently": "^5.3.0",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongoose": "^0.1.0",
    "jsonwebtoken": "^8.5.1",
    "middleware": "^1.0.0",
